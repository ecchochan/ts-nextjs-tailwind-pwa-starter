{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "(cp -n .env.local.example .env.local || true) && node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "dev": "next dev",
    "build": "next build",
    "export": "next build && EXPORTING=true next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "postbuild": "next-sitemap",
    "prepare": "husky install",
    "storybook": "BROWSER=none start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@storybook/addon-postcss": "^2.0.0",
    "clsx": "^1.1.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.2.4",
    "next": "^12.1.0",
    "next-pwa": "^5.4.6",
    "pino": "^7.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1",
    "sass": "^1.49.7",
    "sass-rem": "^3.0.0",
    "tailwind-merge": "^1.2.0",
    "uuid": "^8.3.2",
    "zustand": "^3.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@svgr/webpack": "^6.2.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.136",
    "@tailwindcss/forms": "^0.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/jest-expect-message": "^1.0.3",
    "@types/gtag.js": "^0.0.8",
    "@types/react": "^17.0.39",
    "@types/tailwindcss": "^2.2.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.4",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-expect-message": "^1.0.2",
    "lint-staged": "^11.2.6",
    "next-sitemap": "^1.9.12",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "standard-version": "^9.3.2",
    "storybook-addon-next": "^1.6.2",
    "storybook-addon-sass-postcss": "^0.1.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "resolutions": {
    "webpack": "^5",
    "postcss": "^8.4.7"
  }
}
