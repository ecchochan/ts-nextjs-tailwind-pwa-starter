{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "(cp -n .env.local.example .env.local || true) && node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('You must use Yarn to install, not NPM')\"",
    "dev": "next dev",
    "build": "next build",
    "export": "next build && EXPORTING=true next export",
    "preview-export": "cd out && http-server",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install",
    "storybook": "BROWSER=none start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "^1.1.2",
    "clsx": "^1.2.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "framer-motion": "^7.5.3",
    "http-server": "^14.1.1",
    "next": "^12.3.1",
    "next-pwa": "^5.6.0",
    "pino": "^8.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.49.7",
    "sass-rem": "^3.0.0",
    "tailwind-merge": "^1.6.1",
    "uuid": "^8.3.2",
    "zustand": "^3.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@storybook/addon-actions": "^6.4.18",
    "@storybook/addon-essentials": "^6.4.18",
    "@storybook/addon-links": "^6.4.18",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.4.18",
    "@storybook/manager-webpack5": "^6.4.18",
    "@storybook/react": "^6.4.18",
    "@svgr/webpack": "^6.3.1",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.136",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/gtag.js": "^0.0.8",
    "@types/jest-expect-message": "^1.0.3",
    "@types/react": "^18.0.21",
    "@types/tailwindcss": "^2.2.4",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-storybook": "^0.5.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-expect-message": "^1.1.3",
    "lint-staged": "^13.0.3",
    "next-router-mock": "^0.7.4",
    "next-sitemap": "^3.1.24",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "standard-version": "^9.5.0",
    "storybook-addon-next": "^1.6.2",
    "storybook-addon-sass-postcss": "^0.1.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  },
  "resolutions": {
    "webpack": "^5",
    "postcss": "^8.4.17"
  }
}
